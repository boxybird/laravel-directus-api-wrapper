{
  "name": "boxybird/laravel-directus-api-wrapper",
  "description": "A collection of Laravel facades to interact with Directus CMS.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "directus",
    "api",
    "facade",
    "wrapper"
  ],
  "authors": [
    {
      "name": "Andrew Rhyand",
      "email": "hello@andrewrhyand.com"
    }
  ],
  "require": {
    "laravel/framework": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "BoxyBird\\Directus\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BoxyBird\\Directus\\Providers\\DirectusServiceProvider"
      ]
    }
  }
}
